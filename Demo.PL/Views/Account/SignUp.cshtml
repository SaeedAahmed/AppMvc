@model SignUpViewModel
@{
    ViewData["Tittle"] = "SignUp";
    Layout = "_AythLayout";
}
	<div class="main-agileinfo">
			<div class="agileits-top">
				<form asp-action="SignUp">
					<div asp-validation-summary="All"></div>
			        <input class="text mb-4" type="text" asp-for="FName" placeholder="First Name" required="">
			        <input class="text" type="text" asp-for="LName" placeholder="Last Name" required="">
					<input class="text email" type="email" asp-for="Email"  placeholder="Email" required="">
					<input class="text" type="password" asp-for="Password"  placeholder="Password" required="">
					<input class="text w3lpass" type="password" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
					<div class="wthree-text">
						<label class="anim">
							<input type="checkbox" asp-for="IsAgree" class="checkbox" required="">
							<span>I Agree To The Terms & Conditions</span>
						</label>
						<div class="clear"> </div>
					</div>
					<input type="submit" value="SIGNUP">
				</form>
				<p>Do you have an Account? <a asp-action="SignIn"> Login Now!</a></p>
				<P> <a asp-action="ForgetPassword">Forget Password</a> </P>

			</div>
		</div>
@section ValidateSection
  {
	<partial name="_ValidationScriptsPartial"></partial>
}

